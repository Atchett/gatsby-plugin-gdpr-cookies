  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@testing-library/react": "^9.3.2",
    "babel-preset-gatsby-package": "^0.2.11",
    "cross-env": "^6.0.3"
  },
  "homepage": "https://github.com/andrezimpel/gatsby-plugin-gdpr-cookies#readme",
